#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/prettier@2.6.2/node_modules/prettier/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/prettier@2.6.2/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules:/Users/developer/PhpstormProjects/node_modules:/Users/developer/node_modules:/Users/node_modules:/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/eslint-plugin-prettier@4.0.0_4fe3201cd09a8826bbd8050f2348cb2f/node_modules/eslint-plugin-prettier/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/eslint-plugin-prettier@4.0.0_4fe3201cd09a8826bbd8050f2348cb2f/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/prettier@2.6.2/node_modules/prettier/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/prettier@2.6.2/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules:/Users/developer/PhpstormProjects/node_modules:/Users/developer/node_modules:/Users/node_modules:/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/eslint-plugin-prettier@4.0.0_4fe3201cd09a8826bbd8050f2348cb2f/node_modules/eslint-plugin-prettier/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/eslint-plugin-prettier@4.0.0_4fe3201cd09a8826bbd8050f2348cb2f/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../prettier@2.6.2/node_modules/prettier/bin-prettier.js" "$@"
else
  exec node  "$basedir/../../../../../prettier@2.6.2/node_modules/prettier/bin-prettier.js" "$@"
fi
