#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/ts-node@10.8.0_e54576a33e16b25f43fcd3617c6c86ca/node_modules/ts-node/dist/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/ts-node@10.8.0_e54576a33e16b25f43fcd3617c6c86ca/node_modules/ts-node/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/ts-node@10.8.0_e54576a33e16b25f43fcd3617c6c86ca/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules:/Users/developer/PhpstormProjects/node_modules:/Users/developer/node_modules:/Users/node_modules:/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/node_modules/ts-node/dist/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/node_modules/ts-node/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/ts-node@10.8.0_e54576a33e16b25f43fcd3617c6c86ca/node_modules/ts-node/dist/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/ts-node@10.8.0_e54576a33e16b25f43fcd3617c6c86ca/node_modules/ts-node/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/ts-node@10.8.0_e54576a33e16b25f43fcd3617c6c86ca/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules:/Users/developer/PhpstormProjects/node_modules:/Users/developer/node_modules:/Users/node_modules:/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/node_modules/ts-node/dist/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/node_modules/ts-node/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin.js" "$@"
else
  exec node  "$basedir/../ts-node/dist/bin.js" "$@"
fi
