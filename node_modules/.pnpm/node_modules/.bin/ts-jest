#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/ts-jest@27.1.5_a72543ee8ca9bfae5a9a6d0af9a0f56e/node_modules/ts-jest/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/ts-jest@27.1.5_a72543ee8ca9bfae5a9a6d0af9a0f56e/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules:/Users/developer/PhpstormProjects/node_modules:/Users/developer/node_modules:/Users/node_modules:/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/node_modules/ts-jest/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/ts-jest@27.1.5_a72543ee8ca9bfae5a9a6d0af9a0f56e/node_modules/ts-jest/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/ts-jest@27.1.5_a72543ee8ca9bfae5a9a6d0af9a0f56e/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules:/Users/developer/PhpstormProjects/node_modules:/Users/developer/node_modules:/Users/node_modules:/node_modules:/Users/developer/PhpstormProjects/khmer-transliteration/node_modules/.pnpm/node_modules/ts-jest/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
